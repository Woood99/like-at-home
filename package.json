        "graph-modal": "^1.0.7",
        "graph-tabs": "^1.0.2",
        "gulp-postcss": "^10.0.0",
        "inputmask": "^5.0.7",
        "just-validate": "^3.3.3",
        "marquee3000": "^1.1.1",
        "sass": "^1.75.0",
        "swiper": "^11.1.1"
    }
